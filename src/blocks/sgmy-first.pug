                        .form-block__text
                            p.form-block__main-title Авторизация
                            p.form-block__title Забыли пароль? Восстановить
                        form.form-main(action="", method="post")
                            .login
                                label.label-login(for=".for-email") Логин
                                input.form-email(type="email", name="email" placeholder="Логин" required)
                            .password
                                label.label-password(for=".form-password") Пароль
                                input.form-password(type="password", name="current-password" placeholder="Пароль" required autocomplete="off" )
